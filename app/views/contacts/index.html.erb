<div class="smart-green">

<script>
	$.notify("<%= notice %>", "success");
</script>


/*<h1>Listing <%= @contacts.model.model_name.human(count: 2) %></h1>*/
<br><br>
<h2><%= params[:controller] %></h2>
<br><br>

<table>
  <thead>
    <tr>
      <th><%= @contacts.model.human_attribute_name("name") %></th>
      <th><%= @contacts.model.human_attribute_name("email") %></th>
      <th><%= @contacts.model.human_attribute_name("kind") %></th>
      <th><%= @contacts.model.human_attribute_name("rmk") %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.kind %></td>
        <td><%= contact.rmk %></td>
        <td><%= link_to t('buttons.show'), contact %></td>
        <td><%= link_to t('buttons.edit'), edit_contact_path(contact) %></td>
        <td><%= link_to t('buttons.destroy'), contact, method: :delete, data: { confirm: t('messeges.confirm') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @contacts %>
<br>

<%= link_to 'New Contact', new_contact_path, class: "btn btn-primary" %>
<%= link_to 'Inicio', root_path, class: "btn btn-info" %>
<br><br><br>
<% abc = "ABC" %>
<br><br><br><br><br><br><br><br><br><br><br><br>
<%= render partial: "/contacts/shared/teste", locals: { xyz: abc } %>
</div>
